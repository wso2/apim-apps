        "moment": "^2.29.4",
        "monaco-editor": "^0.21.3",
        "mui-datatables": "^3.8.5",
        "prop-types": "^15.8.1",
        "qs": "^6.11.0",
        "rc-notification": "^4.6.1",
        "react": "^16.14.0",
